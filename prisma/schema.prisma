generator client {
  provider = "prisma-client"
  output = "../generated/prisma"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model product {
  id          String   @id @default(uuid())
  name        String
  price       Decimal  
  createdAt   DateTime @default(now())
  updatedAt   DateTime @updatedAt
}
